
[core]
	editor = emacsclient -c
[user]
	name = Benjamin Lu
	email = ben@trackmaven.com
[alias]
	#cb = !sh /c/apt/aptTools/git/scripts/create-branch
	#maint = !sh /c/apt/aptTools/git/scripts/maint
	#mcp = !sh /c/apt/aptTools/git/scripts/multi-cherry-pick
	#sync = !sh /c/apt/aptTools/git/scripts/sync
	#update-scripts = !sh /c/apt/update-git-scripts
	bd = branch -d
	bl = branch -a
	bn = branch
	bp = push --set-upstream origin HEAD
	ca = commit -a --amend
	ci = commit -am
	cm = commit -m
	co = checkout
	cp = cherry-pick
	cpx = !git cp $* -ex
	d = diff
	dl = diff --name-status
	dh = !sh diffhub
	da = !sh diffall
	dc = !sh diffcommit
	f = !git fetch --tags && git fetch && git submodule init && git submodule update
	gr = log --pretty=format:'%Creset%C(yellow)%h%Creset%C(bold red)%d%Creset %s %Cgreen(%cr) %C(bold cyan)<%an>%Creset' --abbrev --grep
	lg = log --pretty=format:'%Creset%C(yellow)%h%Creset%C(bold red)%d%Creset %s %Cgreen(%cr) %C(bold cyan)<%an>%Creset' --abbrev
	lgg = log --pretty=format:'%Creset%C(yellow)%h%Creset%C(bold red)%d%Creset %s %Cgreen(%cr) %C(bold cyan)<%an>%Creset' --abbrev --graph
	m = !git co master && git p
	p = !git fetch --tags && git pull --rebase && git rp
	pbj = !python -m webbrowser http://youtu.be/s8MDNFaGfT4
	pp = !git p && git push
	rhh = reset --hard HEAD
	rbd = push origin --delete
	rp = fetch --prune
	sps = !git stash && git p && git stash apply
	sss = !git stash && git sync && git stash apply
	st = !git lg -5 && git status
	wipe = !git add . && git reset --hard HEAD
[diff]
	tool = bc3
[difftool]
	bc3 = trustExitCode
[credential]
	helper = osxkeychain
[push]
	default = simple
[merge]
	tool = bc3
[mergetool]
	bc3 = trustExitCode
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
